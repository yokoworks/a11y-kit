{
  "name": "a11y-kit-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "examples/*"
  ],
  "scripts": {
    "build": "echo \"(root) use 'pnpm run build:all' instead\"",
    "build:all": "pnpm -r --parallel run build",
    "lint": "pnpm -r --parallel run lint",
    "typecheck": "pnpm -r --parallel run typecheck",
    "storybook": "pnpm --filter @docs run storybook"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "eslint": "8.57.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.4.20"
  }
}
