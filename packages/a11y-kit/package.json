{
  "name": "@yokoworks/a11y-kit",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.js"
    },
    "./theme/style.css": {
      "default": "./dist/theme/index.css"
    }
  },
  "sideEffects": [
    "*.css",
    "dist/theme/index.css",
    "src/theme/tokens.css",
    "src/theme/index.ts"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/theme/index.ts --dts --format esm --clean",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:ui": "vitest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.19",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "8.57.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "^22.1.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.20",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}
